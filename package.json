{
  "name": "vin-decode",
  "version": "0.1.0",
  "description": "Library for decoding VIN number",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lukasz-pluszczewski/vin-decode.git"
  },
  "main": "lib/vin-decode.js",
  "module": "lib/vin-decode.mjs",
  "jsnext:main": "lib/vin-decode.mjs",
  "scripts": {
    "prepublish": "NODE_PATH=src npm run build",
    "prebuild": "rimraf ./lib",
    "build": "NODE_PATH=src rollup -c",
    "watch": "NODE_PATH=src rollup -c -w",
    "lint": "esw rollup.config.js src --color",
    "test": "npm run mochatest --silent",
    "test:watch": "npm run mochatest -- --watch",
    "mochatest": "NODE_PATH=src mocha tools/testSetup.js \"./test/**/*.js\""
  },
  "keywords": [
    "vin",
    "decode",
    "decoder",
    "vin-decoder"
  ],
  "author": "≈Åukasz Pluszczewski",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-external-helpers": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "babelrc-rollup": "3.0.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "eslint": "3.16.0",
    "eslint-config-brainhub": "1.0.1",
    "eslint-watch": "3.0.0",
    "mocha": "3.2.0",
    "rimraf": "2.6.1",
    "rollup": "0.41.4",
    "rollup-plugin-babel": "2.7.1",
    "sinon": "1.17.7"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/Lukasz-pluszczewski/vin-decode/issues"
